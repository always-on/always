<?xml version="1.0" encoding="UTF-8"?>
<taskModel xmlns="http://www.cs.wpi.edu/~rich/cetask/cea-2018-ext"
           xmlns:disco="urn:disco.wpi.edu:Disco"
           xmlns:a="urn:always.wpi.edu:Activities"
           about="urn:always.wpi.edu:Stranger">
   <task id="_Greetings">
      <subtasks id="__Greetings_subtasks">
         <step name="__Greetings_step" task="disco:Say"/>
         <step name="__Greetings_ref" task="__Greetings_tree"/>
         <binding slot="$__Greetings_step.text"
                  value="&#34;&lt;FACE EXPR=\&#34;SMILE\&#34;/&gt; &lt;DELAY MS=\&#34;10000\&#34;/&gt; &lt;HEADNOD/&gt; &lt;EYEBROWS DIR=UP/&gt; Hello &lt;DELAY MS=\&#34;10000\&#34;/&gt; &lt;FACE EXPR=\&#34;CONCERN\&#34;/&gt; &lt;GAZE horizontal=\&#34;-1\&#34; vertical=\&#34;1\&#34;/&gt; &lt;DELAY MS=\&#34;2000\&#34;/&gt; &lt;GAZE horizontal=\&#34;0\&#34; vertical=\&#34;0\&#34;/&gt; &lt;FACE EXPR=\&#34;WARM\&#34;/&gt; &lt;EYEBROWS DIR=DOWN/&gt; &#34;"/>
         <binding slot="$__Greetings_step.external" value="false"/>
      </subtasks>
   </task>
   <task id="__Greetings_tree">
      <subtasks id="_d4e12_subtasks">
         <step name="_d4e12_step" task="disco:Say"/>
         <binding slot="$_d4e12_step.text" value="&#34;Hi&#34;"/>
         <binding slot="$_d4e12_step.external" value="true"/>
      </subtasks>
   </task>
   <task id="_Session">
      <subtasks id="_s1">
         <step name="greet" task="_Greetings"/>
         <step name="enroll" task="a:EnrollUser" minOccurs="0"/>
         <step name="weather" task="a:DiscussWeather" minOccurs="0"/>
         <step name="explain" task="a:ExplainSelf" minOccurs="0"/>
         <step name="rummy" task="a:PlayRummy" minOccurs="0"/>
      </subtasks>
   </task>
</taskModel>