#!/bin/bash
# 
# print out user model in human-readable form
#
# always-user [USER_DIR USER_FILE]
#
# defaults are ../../../user and User.owl
#
dir="`dirname $0`"
cd $dir
java -cp "../class;../../discoRT/class;../../../user;../../../core/discoRT/lib/picocontainer-2.14.1.jar;../../../core/discoRT/lib/joda-time-2.0.jar;../../../core/discoRT/lib/disco1.6.10/lib/disco.jar;../lib/log4j-1.2.17.jar;../lib/owlapi/owlapi-bin.jar;../lib/owlapi/pellet-rules.jar;../lib/owlapi/pellet-datatypes.jar;../lib/owlapi/pellet-core.jar;../lib/owlapi/jgrapht-jdk1.5.jar;../lib/owlapi/aterm-java-1.6.jar;../lib/owlapi/pellet-owlapiv3.jar;../lib/owlapi/pellet-el.jar" edu.wpi.always.user.UserUtils "$@"
