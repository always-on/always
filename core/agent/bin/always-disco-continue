#!/bin/bash
#
# Usage: always-disco-continue [ <filename> ] [ <cp1> <cp2> ...]
#
# Runs the given script file using Always.Disco and compares resulting temporary
# log with corresponding test log file <filename>.test, if it exists
# 
# Note that cp's are relative to location of _run below!
#
# NB: Unlike always-disco, this command does *not* call always-restore-user
#     to restore always/user/TestUser.owl to its original value.
#
dir="`dirname $0`"
filename=$1
shift
"$dir/../../discoRT/lib/disco1.6.10/bin/_run" edu.wpi.always.Always\$Disco "$filename" "$@" ../../../../core/discoRT/lib/picocontainer-2.14.1.jar ../../../../core/discoRT/lib/joda-time-2.0.jar ../../../../user ../../../../core/agent/class ../../../../core/discoRT/class lib/disco.jar ../../../../core/agent/lib/log4j-1.2.17.jar ../../../../core/agent/lib/owlapi/owlapi-bin.jar ../../../../core/agent/lib/owlapi/pellet-rules.jar ../../../../core/agent/lib/owlapi/pellet-datatypes.jar ../../../../core/agent/lib/owlapi/pellet-core.jar ../../../../core/agent/lib/owlapi/jgrapht-jdk1.5.jar ../../../../core/agent/lib/owlapi/aterm-java-1.6.jar ../../../../core/agent/lib/owlapi/pellet-owlapiv3.jar ../../../../core/agent/lib/owlapi/pellet-el.jar
